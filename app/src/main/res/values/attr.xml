<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FreeArrowView">
        <attr name="orientation">
            <enum name="left" value="0" />
            <enum name="up" value="1" />
            <enum name="right" value="2" />
            <enum name="down" value="3" />
        </attr>
        <attr name="withPole" format="boolean" />
        <attr name="arrowColor" format="color" />
        <attr name="poleRatio" format="float" />
        <attr name="poleWidth" format="dimension" />
        <attr name="lineStrokeWidth" format="dimension" />
        <attr name="fillArrow" format="boolean" />
        <attr name="arrowThick" format="dimension" />
        <attr name="dashPole" format="boolean" />
        <attr name="dashGap" format="dimension" />
        <attr name="dashWidth" format="dimension" />
    </declare-styleable>
</resources>